{
  "name": "mouse-mcp",
  "version": "1.0.0",
  "description": "MCP server for Disney Parks data",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mouse-mcp": "dist/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "test": "vitest",
    "test:run": "vitest run",
    "inspector": "npx @modelcontextprotocol/inspector tsx src/index.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "@xenova/transformers": "^2.17.0",
    "fuse.js": "^7.0.0",
    "playwright": "^1.49.1",
    "sql.js": "^1.11.0"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.33.1",
    "@types/node": "^22.10.2",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "mcp",
    "disney",
    "theme-parks",
    "walt-disney-world",
    "disneyland"
  ],
  "author": "Cameron",
  "license": "MIT"
}
