{
  "name": "mouse-mcp",
  "version": "1.0.0",
  "description": "MCP server for Disney Parks data",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mouse-mcp": "dist/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "check": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "test": "vitest",
    "test:run": "vitest run",
    "validate": "npm run check && npm run lint && npm run format:check && npm run test:run",
    "inspector": "npx @modelcontextprotocol/inspector tsx src/index.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@lancedb/lancedb": "^0.22.3",
    "@modelcontextprotocol/sdk": "^1.12.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.2",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@sentry/node": "^8.55.0",
    "@xenova/transformers": "^2.17.0",
    "dotenv": "^17.2.3",
    "fuse.js": "^7.0.0",
    "playwright": "^1.49.1",
    "sql.js": "^1.11.0"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.33.1",
    "@eslint/js": "^9.39.1",
    "@types/node": "^22.10.2",
    "@types/sql.js": "^1.4.9",
    "eslint": "^9.16.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "mcp",
    "disney",
    "theme-parks",
    "walt-disney-world",
    "disneyland"
  ],
  "author": "Cameron",
  "license": "MIT"
}
